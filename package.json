{
  "name": "flex-living-backend",
  "version": "1.0.0",
  "description": "Reviews Dashboard Backend for Flex Living",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "seed": "ts-node scripts/seed.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --testTimeout=30000 --setupFilesAfterEnv ./src/tests/setup.ts",
    "test:unit": "cross-env NODE_ENV=test jest src/tests/unit --runInBand --testTimeout=30000 --setupFilesAfterEnv ./src/tests/setup.ts",
    "test:integration": "cross-env NODE_ENV=test jest src/tests/integration --runInBand --testTimeout=60000 --setupFilesAfterEnv ./src/tests/setup.ts",
    "test:e2e": "cross-env NODE_ENV=test jest src/tests/e2e --runInBand --testTimeout=60000 --setupFilesAfterEnv ./src/tests/setup.ts",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --testTimeout=30000",
     "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --runInBand --testTimeout=30000 --reporters=default --reporters=jest-junit",
    "test:coverage:ci": "cross-env NODE_ENV=test jest --ci --coverage --runInBand --testTimeout=30000 --coverageReporters=text-summary --coverageReporters=lcov"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "mongoose": "^7.6.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.27",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "cross-env": "^10.1.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^11.0.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
